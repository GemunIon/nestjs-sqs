{
  "collectCoverageFrom": ["lib/**/*.{js,jsx,tsx,ts}", "!**/node_modules/**", "!**/*.spec.ts"],
  "coverageDirectory": "coverage/e2e",
  "coverageReporters": ["lcov"],
  "moduleFileExtensions": ["ts", "js", "json"],
  "preset": "ts-jest",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  }
}
